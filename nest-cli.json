{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/lavida-backend/src",
  "monorepo": true,
  "root": "apps/lavida-backend",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/lavida-backend/tsconfig.app.json"
  },
  "projects": {
    "lavida-backend": {
      "type": "application",
      "root": "apps/lavida-backend",
      "entryFile": "main",
      "sourceRoot": "apps/lavida-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/lavida-backend/tsconfig.app.json"
      }
    },
    "judge": {
      "type": "application",
      "root": "apps/judge",
      "entryFile": "main",
      "sourceRoot": "apps/judge/src",
      "compilerOptions": {
        "tsConfigPath": "apps/judge/tsconfig.app.json"
      }
    }
  }
}